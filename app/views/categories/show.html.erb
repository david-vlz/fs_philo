<div class="row-fluid">
	<div class="span2">
		sidebar-content
	</div>
	<div class="span10">
		<% @articles.each do |article| %>
			<section class="well">
				<h3><%=link_to(article.title, article_path(@id=article.id))%></h3>
				<p class="lead"><%=(article.body[0...266]+"...").html_safe%></p>
			</section>
		<% end %>
	</div>
</div>
