			<div id="header" class="row-fluid" style="position:relative">
				<div class="span10">
					<header class="center page-header">
						<h1>
							<%= link_to("Fachschaft Philosophie", root_path) %>
							<small><%= link_to("Universit&auml;t zu K&ouml;ln".html_safe, "http://www.uni-koeln.de/") %></small>
						</h1>
						
					</header>
				</div>
				<div class="span2 logo">
					<%= link_to(image_tag("/philos.png"), root_path) %>
				</div>
			</div>

			<div class="row-fluid">
				<div class="navbar span12">
					<div class="navbar-inner">
						
						 <a class="btn btn-navbar pull-left" data-toggle="collapse" data-target=".nav-collapse">
							<div class="pull-right">
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
							</div>
							<div class="pull-right" style="margin-right:10px">
								&Uuml;bersicht
							</div>
						</a>

						<div class="nav-collapse collapse">
							<ul class="nav">
								<li><%= link_to("Home", root_path) %></li>
								<% fetchCategories.each do |category| %>
									<% if category.visible? %>
										<li><%= link_to(category.name, category_path(category)) %></li>
									<% elsif signed_in? %>
										<li><%= link_to(category.name, category_path(category), class: 'muted') %></li>
									<% end %>
								<% end %>
							</ul>
						</div>


						<% if signed_in? %>
							<ul class="nav pull-right">
								<li class="dropdown">
									<a href="#" class="dropdown-toggle" data-toggle="dropdown">
										<i class="icon-cog icon-white"></i>
										<b class="caret"></b>
									</a>
									<ul class="dropdown-menu">
										<li><%= link_to('Alle Benutzer', users_path) %></li>
										<li><%= link_to('Profil', current_user) %></li>
										<li><%= link_to('Neue Seite', new_category_path) %></li>
										<li><%= link_to('Logout', logout_path, method: "delete") %></li>
									</ul>
								</li>
							</ul>
						<% else %>
							<ul class="nav pull-right">
								<li><%= link_to('Login', login_path) %></li>
							</ul>
						<% end %>
					</div>
				</div>
			</div>
